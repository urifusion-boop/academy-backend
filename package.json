    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.8",
    "pino": "^9.4.0",
