    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.3.3",
    "prisma": "^5.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.6.3"
